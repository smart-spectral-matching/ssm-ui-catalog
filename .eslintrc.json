{
  "extends": ["react-app", "eslint:recommended", "prettier"],
  "rules": {
    "no-case-declarations": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "react-hooks/exhaustive-deps": 0,
    "no-console": "warn",
    "complexity": ["error", 10]
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
