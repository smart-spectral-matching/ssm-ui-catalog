{
  "name": "smart-spectral-matching",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "homepage": "http://ssm-dev.ornl.gov",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "format": "prettier --write . && eslint --color --fix .",
    "assert-no-warnings": "eslint --max-warnings 0 --color .",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.4.0",
    "d3": "^7.6.1",
    "d3-color": "^3.1.0",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "mobx-utils": "^6.0.4",
    "nanoid": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.19.0",
    "react-router-dom": "^5.2.0",
    "web-vitals": "^1.1.2",
    "workbox-core": "^6.3.0",
    "workbox-expiration": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-routing": "^6.3.0",
    "workbox-strategies": "^6.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/d3": "^7.1.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-no-unsanitized": "^3.1.5",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "react-scripts": "5.0.1",
    "sass": "^1.35.2",
    "typescript": "^4.3.5"
  },
  "overrides": {
    "recursive-readdir@2.2.2": {
      "minimatch@3.0.4": "3.1.2"
    }
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 15
      }
    }
  },
  "lint-staged": {
    "**/*.{html,md,css,sass,scss,json,yaml,yml}": [
      "prettier --write"
    ],
    "**/*.(ts|js)?(x)": [
      "prettier --write",
      "eslint --color --fix --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      "> 0.5%",
      "last 2 versions",
      "Firefox ESR",
      "not dead",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16"
  }
}
